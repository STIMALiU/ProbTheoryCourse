\begin_inset Formula $[a,b]$
\end_inset

)
\end_layout

\begin_deeper
\begin_layout Enumerate
Derive the density of 
\begin_inset Formula $Z_{n}=\mathrm{max}(X_{1},X_{2},...,X_{n}$
\end_inset

).
\begin_inset space \hspace{}
\length 1cm
\end_inset

2p.
\end_layout

\begin_layout Enumerate
Let 
\begin_inset Formula $Y_{n}=\frac{1}{n}\sum_{k=1}^{n}X_{k}$
\end_inset

.
 Show that 
\begin_inset Formula $Y_{n}\overset{p}{\rightarrow}\frac{1}{2}$
\end_inset

 as 
\begin_inset Formula $n\rightarrow\infty.$
\end_inset

 
\begin_inset space \hspace{}
\length 1cm
\end_inset

1p.
\end_layout

\begin_layout Enumerate
Let 
\begin_inset Formula $W_{n}$
\end_inset

 be a sequence of random variables with finite mean 
\begin_inset Formula $\mu$
\end_inset

 and variance 
\begin_inset Formula $\sigma^{2}$
\end_inset

.
 Let 
\begin_inset Formula $\bar{W}_{n}=\frac{1}{n}\sum_{i=1}^{n}W_{i}$
\end_inset

.
 Show that 
\begin_inset Formula $\bar{W}_{n}\cdot Y_{n}$
\end_inset

 converges in distribution as 
\begin_inset Formula $n\rightarrow\infty$
\end_inset

, and find the limiting distribution.
 
\begin_inset space \hspace{}
\length 1cm
\end_inset

2p.
\end_layout

\end_deeper
